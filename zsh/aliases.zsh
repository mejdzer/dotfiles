alias reload!='. ~/.zshrc'
alias ll='ls -la'
alias d2u='find . -exec dos2unix {} \;'
alias pwd='pwd && pwd | pbcopy'
alias mamp="sudo vim ~/Sites/_config"
alias tunel="vim ~/.ssh"
alias mysql='mysql -u root -proot'
alias mysqldump='mysqldump5 -u root -proot'
alias h5bp='git clone git://github.com/h5bp/html5-boilerplate.git h5bp && cd h5bp'
alias clearlogs="sudo rm -rf /private/var/log/asl/*"
alias eali="vim -O ~/.dotfiles/zsh/aliases.zsh ~/.dotfiles/git/aliases.zsh"
alias vali="cat ~/.dotfiles/zsh/aliases.zsh ~/.dotfiles/git/aliases.zsh"
alias weather="curl -4 wttr.in/New_York"
alias ct="ccat"
alias webtermsrv="gotty tmux new -A -s gotty top"
alias webtermcli="tmux new -A -s gotty"
alias ossl="/usr/local/opt/openssl/bin/openssl"
alias r2="rvm use --default ruby-2.4.2@global"
alias php56="brew unlink php && brew link --force php@5.6"
alias php72="brew unlink php@5.6 && brew link --force php"
